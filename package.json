{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.16",
  "author": "LekoArts",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "products:status": "node scripts/manage-direct-products.js status",
    "products:seed:direct": "node scripts/manage-direct-products.js seed",
    "products:add:direct": "node scripts/manage-direct-products.js add",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "node scripts/verify-main-route.js",
    "check": "npm run lint && npm run test",
    "postinstall": "if [ \"$NETLIFY\" = \"true\" ]; then npm rebuild sharp --platform=linux --arch=x64; fi"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource-variable/space-grotesk": "^5.1.0",
    "@lekoarts/gatsby-theme-minimal-blog": "^6.2.1",
    "@react-three/drei": "^9.0.0",
    "@react-three/fiber": "^9.0.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@theme-ui/color-modes": "^0.16.2",
    "@theme-ui/components": "^0.16.2",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "framer-motion": "^12.9.2",
    "gatsby": "^5.13.6",
    "gatsby-plugin-algolia": "^1.0.3",
    "gatsby-plugin-feed": "^5.13.1",
    "gatsby-plugin-google-analytics": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "gatsby-remark-mermaid": "^5.0.0",
    "gsap": "^3.14.2",
    "mermaid": "^11.12.1",
    "netlify-identity-widget": "^1.9.2",
    "pg": "^8.13.1",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.33.4",
    "stripe": "^15.12.0",
    "theme-ui": "^0.16.2",
    "three": "^0.181.0",
    "typeface-montserrat": "^1.1.13",
    "use-sync-external-store": "^1.6.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.181.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.5.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript-eslint": "^7.18.0"
  }
}
